<AnchorPane fx:id="anchorPane" prefHeight="400.0" prefWidth="716.0" styleClass="bodybg" stylesheets="@../backgrounds/listBackground.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.customers.ProductList">
