            <Font size="11.0" />
         </font>
      </Label>
      <Label fx:id="labelInfo" alignment="CENTER" layoutX="566.0" layoutY="136.0" opacity="0.6" prefHeight="40.0" prefWidth="136.0" style="-fx-background-color: blue;" textAlignment="CENTER" textFill="WHITE" />
      <Button fx:id="btnChangeList" layoutX="564.0" layoutY="22.0" mnemonicParsing="false" onAction="#changeList" prefHeight="55.0" prefWidth="136.0" stylesheets="@../backgrounds/buttonPressed2.css" text="Available List" textFill="WHITE">
